  },
  "devDependencies": {
    "@types/node": "18.14.0",
    "@types/semver": "7.3.13",
    "@typescript-eslint/parser": "5.52.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.34.0",
    "eslint-plugin-github": "4.6.1",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.3",
    "js-yaml": "4.1.0",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
